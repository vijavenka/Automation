          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Check if departments are the same" enabled="true">
            <stringProp name="BeanShellAssertion.query">
Integer counter=0;

for( i=1;i&lt;=Integer.parseInt(vars.get(&quot;indiviualDepartmentFromRedemptions_matchNr&quot;)); i++)
{
	for( j=1;j&lt;=Integer.parseInt(vars.get(&quot;individualDepartmentFromDepartmants_matchNr&quot;)); j++)
	{
		String dept1 =vars.get(&quot;indiviualDepartmentFromRedemptions_&quot;+i);
		String dept2 =vars.get(&quot;individualDepartmentFromDepartmants_&quot;+j);

		if( dept1.equals(dept2) )
		{
			counter = counter +1;
		}
	}
}


		if(  Integer.parseInt(vars.get(&quot;indiviualDepartmentFromRedemptions_matchNr&quot;)) != counter )
		{
			
			Failure = true;
			FailureMessage = &quot;some departments are different or they number is different - redemptions &quot; + vars.get(&quot;indiviualDepartmentFromRedemptions_matchNr&quot;) + &quot; departments &quot; + vars.get(&quot;individualDepartmentFromDepartmants_matchNr&quot;) + &quot;  counter &quot; + counter;
			print (FailureMessage);   // this goes to stdout
			log.warn(FailureMessage); // this goes to the JMeter log file
			break;
		
		}else{
			Failure = false;
		}

</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
            <stringProp name="BeanShellAssertion.parameters"></stringProp>
            <boolProp name="BeanShellAssertion.resetInterpreter">false</boolProp>
