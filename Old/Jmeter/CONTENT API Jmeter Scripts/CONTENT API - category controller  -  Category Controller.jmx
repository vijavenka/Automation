              <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Check if types are the same" enabled="true">
                <stringProp name="BeanShellAssertion.query">
Integer counter=0;
String tp1;
String tp2;

for( i=1;i&lt;=Integer.parseInt(vars.get(&quot;indiviualTypeFromRedemptions_matchNr&quot;)); i++)
{
	counter = 0;
	tp1 =vars.get(&quot;indiviualTypeFromRedemptions_&quot;+i);

	for( j=1;j&lt;=Integer.parseInt(vars.get(&quot;individualTypeFromCategory_matchNr&quot;)); j++)
	{
		tp2 =vars.get(&quot;individualTypeFromCategory_&quot;+j);

		if( tp1.equals(tp2) )
		{
			counter = 1;
		}
	}

		if(  counter == 0 )
		{
			
			Failure = true;
