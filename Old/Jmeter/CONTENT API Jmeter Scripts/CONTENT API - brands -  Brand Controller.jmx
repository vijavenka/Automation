              <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Check if brand name contains proper substring" enabled="true">
                <stringProp name="BeanShellAssertion.query">
String substr;
String ifFound =&quot;false&quot;;
String brand;


	for( i=1;i&lt;=Integer.parseInt(vars.get(&quot;individualBrandNameFromBrandsSubstring_matchNr&quot;)); i++)
	{
		substr = vars.get(&quot;brandNameSubstring&quot;);
		brand = vars.get(&quot;individualBrandNameFromBrandsSubstring_&quot;+i);
		
		if(  (brand.toLowerCase()).contains(substr) )
		{
			ifFound = &quot;true&quot;;

		}else{
			if(  (vars.get(&quot;brandsSynonymsFromBrandsSubstring_&quot;+i).toLowerCase()).contains(substr) )
			{
				ifFound = &quot;true&quot;;

			}else{
			}
		}
	}



	if( ifFound.equals(&quot;true&quot;) )
	{
		Failure = false;
	}else{
		Failure = true;
		FailureMessage = &quot;Brand name &quot; + brand + &quot; does not contains phrase &quot; + &quot;-&quot;+substr+&quot;-&quot;;
		print (FailureMessage);   // this goes to stdout
		log.warn(FailureMessage); // this goes to the JMeter log file
		break;
	}



</stringProp>
