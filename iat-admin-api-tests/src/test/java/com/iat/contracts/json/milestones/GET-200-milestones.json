{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "$id": "http://example.com/example.json",
  "additionalProperties": false,
  "required": [
    "valueType",
    "values"
  ],
  "properties": {
    "valueType": {
      "type": "string",
      "$id": "/properties/valueType"
    },
    "values": {
      "type": "array",
      "$id": "/properties/values",
      "additionalItems": false,
      "items": {
        "type": "object",
        "$id": "/properties/values/items",
        "additionalProperties": false,
        "required": [
          "id",
          "value"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "$id": "/properties/values/items/properties/id"
          },
          "value": {
            "type": "integer",
            "$id": "/properties/values/items/properties/value"
          }
        }
      }
    }
  }
}