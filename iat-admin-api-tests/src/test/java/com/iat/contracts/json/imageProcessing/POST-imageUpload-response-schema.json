{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "POST image upload",
  "description": "Object returned with /api/image POST request",
  "type": "object",
  "properties": {
    "imageUrl": {
      "id": "imageUrl",
      "type": "string"
    },
    "thumbnailUrl": {
      "id": "thumbnailUrl",
      "type": [
        "string",
        "null"
      ]
    },
    "warningMessage": {
      "id": "warningMessage",
      "type": "string"
    }
  },
  "required": [
    "imageUrl"
  ]
}