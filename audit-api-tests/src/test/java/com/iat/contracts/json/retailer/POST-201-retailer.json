{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "retailerName": {
      "type": [
        "string",
        "null"
      ]
    },
    "email": {
      "type": "string"
    },
    "epointsUuid": {
      "type": "string"
    },
    "wholesalerId": {
      "type": [
        "integer",
        "null"
      ]
    },
    "chains": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "chainName": {
            "type": [
              "string",
              "null"
            ]
          },
          "parent": {
            "type": [
              "string",
              "null"
            ]
          },
          "groupId": {
            "type": [
              "integer",
              "null"
            ]
          },
          "partner_short_name": {
            "type": "string"
          },
          "partner_api_key": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "chainName",
          "parent",
          "groupId",
          "partner_short_name",
          "partner_api_key"
        ]
      }
    }
  },
  "required": [
    "id",
    "retailerName",
    "email",
    "epointsUuid",
    "wholesalerId",
    "chains"
  ]
}