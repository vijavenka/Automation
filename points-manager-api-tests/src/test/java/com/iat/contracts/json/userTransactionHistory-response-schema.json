{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User Transactions",
  "description": "Object returned when /transactions/client/{clientId}/ used",
  "type": "object",
  "properties": {
    "totalResults": {
      "id": "totalResults",
      "type": "integer"
    },
    "searchResultsCount": {
      "id": "searchResultsCount",
      "type": "integer"
    },
    "transactions": {
      "id": "transactions",
      "type": "array",
      "items": {
        "id": "transactions",
        "type": "object",
        "properties": {
          "transactionUri": {
            "id": "transactionUri",
            "type": "string"
          },
          "transactionDate": {
            "id": "transactionDate",
            "type": "integer"
          },
          "status": {
            "id": "status",
            "type": "string"
          },
          "delta": {
            "id": "delta",
            "type": "integer"
          },
          "externalTransactionId": {
            "id": "externalTransactionId",
            "type": [
              "null",
              "string"
            ]
          },
          "site": {
            "id": "site",
            "type": [
              "null",
              "string"
            ]
          },
          "siteUrl": {
            "id": "siteUrl",
            "type": [
              "null",
              "string"
            ]
          },
          "tagName": {
            "id": "tagName",
            "type": [
              "null",
              "string"
            ]
          },
          "tagImageUrl": {
            "id": "tagImageUrl",
            "type": [
              "null",
              "string"
            ]
          },
          "balance": {
            "id": "balance",
            "type": "integer"
          },
          "onBehalfOfId": {
            "id": "onBehalfOfId",
            "type": [
              "null",
              "string"
            ]
          },
          "activityInfo": {
            "id": "activityInfo",
            "type": [
              "null",
              "string"
            ]
          },
          "reasonText": {
            "id": "reasonText",
            "type": [
              "null",
              "string"
            ]
          },
          "autoConfirmDate": {
            "id": "autoConfirmDate",
            "type": [
              "null",
              "integer"
            ]
          }
        }
      }
    }
  },
  "required": [
    "totalResults",
    "searchResultsCount",
    "transactions"
  ]
}