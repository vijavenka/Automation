{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Get partner balance",
  "description": "Object returned with get partner transactions request",
  "type": "object",
  "properties": {
    "declinedPoints": {
      "type": "integer",
      "minimum": 0
    },
    "clientId": {
      "type": "string"
    },
    "spentPoints": {
      "type": "integer",
      "minimum": 0
    },
    "redeemedPoints": {
      "type": "integer",
      "minimum": 0
    },
    "confirmedPoints": {
      "type": "integer",
      "minimum": 0
    },
    "pendingPoints": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "clientId"
  ]
}