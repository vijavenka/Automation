{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "seoSlug": {
      "type": "string"
    },
    "epoints": {
      "type": "object",
      "properties": {
        "UK": {
          "type": "integer"
        }
      },
      "required": [
        "UK"
      ]
    },
    "retailer": {
      "type": "null"
    },
    "imageUrl": {
      "type": "string"
    },
    "thumbnailUrl": {
      "type": "null"
    },
    "promoted": {
      "type": "boolean"
    },
    "description": {
      "type": "string"
    },
    "merchantName": {
      "type": "string"
    },
    "merchantId": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "brand": {
      "type": [
        "null",
        "string"
      ]
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "localCurrencyValue": {
      "type": "null"
    },
    "currency": {
      "type": "string"
    },
    "conversionRate": {
      "type": "null"
    }
  },
  "required": [
    "id",
    "title",
    "seoSlug",
    "epoints",
    "retailer",
    "imageUrl",
    "thumbnailUrl",
    "promoted",
    "description",
    "merchantName",
    "merchantId",
    "department",
    "brand",
    "categories",
    "localCurrencyValue",
    "currency",
    "conversionRate"
  ]
}