{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TagDetails",
  "description": "Object returned when tag/{tagKey} used",
  "type": "object",
  "properties": {
    "tagName": {
      "type": "string"
    },
    "cap": {
      "type": "integer",
      "minimum": 0
    },
    "frequency": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "autoConfirm": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "tagName",
    "cap",
    "frequency",
    "imageUrl",
    "description",
    "autoConfirm"
  ]
}