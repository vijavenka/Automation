{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Get awarded points for given partner",
  "description": "Object returned awarded points for partner",
  "type": "object",
  "properties": {
    "allCount": {
      "type": "integer",
      "minimum": 0
    },
    "pageSize": {
      "type": "integer",
      "minimum": 0
    },
    "pagesCount": {
      "type": "integer",
      "minimum": 0
    },
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string"
          },
          "amount": {
            "type": "integer"
          },
          "value": {
            "type": "number"
          },
          "currency": {
            "type": "string"
          },
          "issuedBy": {
            "type": "object",
            "properties": {
              "email": {
                "type": "string"
              }
            },
            "required": [
              "email"
            ]
          },
          "awardedTo": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "email": {
                "type": "string"
              },
              "firstName": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "lastName": {
                "type": [
                  "string",
                  "null"
                ]
              }
            },
            "required": [
              "id",
              "email"
            ]
          },
          "reason": {
            "type": "string"
          },
          "tagName": {
            "type": "string"
          }
        },
        "required": [
          "date",
          "amount",
          "value",
          "currency",
          "issuedBy",
          "awardedTo",
          "reason",
          "tagName"
        ]
      }
    },
    "totals": {
      "id": "totals",
      "type": "object",
      "properties": {
        "amount": {
          "type": "integer"
        },
        "value": {
          "type": "number"
        },
        "currency": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "allCount",
    "pageSize",
    "pagesCount",
    "results",
    "totals"
  ]
}