{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Partners site not visited",
  "description": "Object returned for user when request for partners site not visited",
  "type": "object",
  "properties": {
    "partnersToEarn": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "siteUrl": {
            "type": [
              "string",
              "null"
            ],
            "format": "uri"
          },
          "description": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "name",
          "siteUrl",
          "description"
        ]
      }
    },
    "totalResults": {
      "type": "integer",
      "minimum": 0
    },
    "searchResultsCount": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "partnersToEarn",
    "totalResults",
    "searchResultsCount"
  ]
}