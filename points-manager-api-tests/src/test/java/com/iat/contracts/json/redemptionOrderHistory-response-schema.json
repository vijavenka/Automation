{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Get redemption order history",
  "description": "Object returned user redemption order history",
  "type": "object",
  "properties": {
    "totalResults": {
      "type": "integer"
    },
    "searchResultsCount": {
      "type": "integer"
    },
    "rewards": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "orderId": {
            "type": "string"
          },
          "productId": {
            "type": "string"
          },
          "imageUrl": {
            "type": "string",
            "format": "uri"
          },
          "title": {
            "type": "string"
          },
          "numPoints": {
            "type": "integer"
          },
          "quantity": {
            "type": "integer"
          },
          "merchantName": {
            "type": "string"
          },
          "createdAt": {
            "type": "integer"
          },
          "gift": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          },
          "address1": {
            "type": "string"
          },
          "address2": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "county": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "postcode": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "zone": {
            "type": "string"
          }
        },
        "required": [
          "orderId",
          "productId",
          "imageUrl",
          "title",
          "numPoints",
          "quantity",
          "merchantName",
          "createdAt",
          "gift",
          "name",
          "address1",
          "address2",
          "city",
          "county",
          "country",
          "postcode",
          "phone",
          "zone"
        ]
      }
    }
  },
  "required": [
    "totalResults",
    "searchResultsCount",
    "rewards"
  ]
}